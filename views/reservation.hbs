{{#if data1}}

{{#each data1}}
<form name="Cancel" method="post" action="/cancel">
<div class="container">
   <div class="row">
      <div class="col-md-12">
         <div class="card flex-md-row mb-4 shadow-sm h-md-250">
          <img class="card-img-left flex-auto d-none d-lg-block" alt="Thumbnail [300x350]" src="{{image}}" style="width: 300px; height: 250px;">
            <div class="card-body d-flex flex-column align-items-start" style="background-color:  #F8F9F9 ;">
               <strong class="d-inline-block mb-2 text-primary" style="text-align: center;">{{resname}}</strong>
               <input type="hidden" value="{{resid}}" name="RESID"> <br>
               <h6 class="mb-0">
                   <p class="card-text mb-auto"> Cuisine : {{cuisine}}</p>
               </h6>
               
               <p class="card-text mb-auto"> Address : {{address}}</p>
               <p class="card-text mb-auto"> {{city}}</p>
               <p class="card-text mb-auto"> Reservation made by {{name}} for {{seats}} on {{date}} at {{time}}</p>
               <div class="mb-1 text-muted small">Phone No :{{phno}}</div>
                 <!-- <div class="mb-1 text-muted small">Timings :{{opentime}} - {{closetime}}</div> -->
               <div align="center">
               	  <input type="hidden" name="cancel" value="{{booking_id}}">
                  <input type="submit" class="btn btn-info" value="Cancel Reservation" >
              </div>
            </div>
         </div>
      </div>
    </div>
  </div>
</form>
 {{/each}}

 {{else}}
  <p style="text-align: center;color: blue;"> No Reservations as of now </p>

  {{/if}}

